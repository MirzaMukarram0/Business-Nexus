{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "MONGO_URI": "MONGO_URI",
    "JWT_SECRET": "JWT_SECRET",
    "CORS_ORIGIN": "CORS_ORIGIN",
    "PORT": "5000"
  }
} 